{
  "name": "@limits-trade/limits-sdk",
  "version": "1.0.3",
  "description": "TypeScript SDK for Limits API - Trading and account management",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "trading",
    "api",
    "sdk",
    "hyperliquid",
    "typescript",
    "limits"
  ],
  "author": "Limits.trade Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/limits-trade/limits-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/limits-trade/limits-sdk/issues"
  },
  "homepage": "https://limits.trade",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "rollup": "^3.29.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "ethers": "^6.15.0"
  },
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
